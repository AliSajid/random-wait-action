    "devDependencies": {
        "@eslint/js": "9.39.2",
        "@rollup/plugin-commonjs": "29.0.0",
        "@rollup/plugin-node-resolve": "16.0.3",
        "@rollup/plugin-typescript": "12.3.0",
        "@types/eslint": "9.6.1",
        "@types/node": "22.19.9",
        "@vercel/ncc": "0.38.4",
        "@vitest/coverage-v8": "4.0.18",
        "@vitest/eslint-plugin": "1.6.6",
        "@vitest/ui": "4.0.18",
        "eslint": "9.39.2",
        "fast-check": "4.5.3",
        "globals": "17.2.0",
        "prettier": "3.8.1",
        "prettier-plugin-multiline-arrays": "4.1.3",
        "rollup": "4.57.1",
        "tslib": "2.8.1",
        "typescript": "5.9.3",
        "typescript-eslint": "8.54.0",
        "vitest": "4.0.18"
    },
    "keywords": [
        "actions",
        "node",
        "setup"
    ],
    "license": "MIT",
