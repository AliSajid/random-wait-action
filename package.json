{
  "author": "Ali Sajid Imami",
  "dependencies": {
    "@actions/core": "^1.11.1"
  },
  "description": "A Github Action to introduce a random delay in a Job",
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.17.0",
    "@stylistic/eslint-plugin": "2.12.1",
    "@types/eslint__js": "8.42.3",
    "@types/jest": "29.5.14",
    "@types/node": "22.9.1",
    "@vercel/ncc": "0.38.3",
    "eslint": "9.17.0",
    "eslint-plugin-jest": "28.9.0",
    "globals": "15.14.0",
    "jest": "29.7.0",
    "js-yaml": "4.1.0",
    "jsdoc-to-markdown": "9.1.1",
    "prettier": "3.4.2",
    "prettier-plugin-multiline-arrays": "4.0.1",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.7.2",
    "typescript-eslint": "8.18.1"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "license": "MIT",
  "main": "lib/main.js",
  "name": "random-wait-action",
  "packageManager": "pnpm@9.15.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AliSajid/random-wait-action"
  },
  "scripts": {
    "all": "pnpm run build && pnpm run format && pnpm run lint && pnpm run package && pnpm test",
    "build": "tsc",
    "document": "jsdoc2md src -r -d docs/docs",
    "format": "prettier --write '{src,docs}/**/*.{ts,js,json,md,yaml,yml}'",
    "format-check": "prettier --check '{src,docs}/**/*.{ts,js,json,md,yaml,yml}'",
    "lint": "eslint",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "version": "2.5.0-next.1"
}
